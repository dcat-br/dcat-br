{
  "version": "1.0.0",
  "maxScore": 405,
  "maxCompletude": 130,
  "maxTotalFairQC": 535,
  "dimensions": {
    "encontrabilidade": {
      "maxScore": 100,
      "label": "Encontrabilidade",
      "label_en": "Findability"
    },
    "acessibilidade": {
      "maxScore": 100,
      "label": "Acessibilidade",
      "label_en": "Accessibility"
    },
    "interoperabilidade": {
      "maxScore": 110,
      "label": "Interoperabilidade",
      "label_en": "Interoperability"
    },
    "reutilizabilidade": {
      "maxScore": 75,
      "label": "Reutilizabilidade",
      "label_en": "Reusability"
    },
    "contextualidade": {
      "maxScore": 20,
      "label": "Contextualidade",
      "label_en": "Contextuality"
    }
  },
  "classifications": {
    "Sem Selo": {
      "minTotal": 0,
      "maxTotal": 203,
      "completude": 83,
      "label": "Sem Selo (Insuficiente)",
      "criterio": "Total < 204"
    },
    "Bronze": {
      "minTotal": 204,
      "maxTotal": 303,
      "completude": 83,
      "label": "Bronze (Suficiente)",
      "criterio": "Total >= 204"
    },
    "Prata": {
      "minTotal": 304,
      "maxTotal": 470,
      "completude": 125,
      "label": "Prata (Bom)",
      "criterio": "Total >= 304"
    },
    "Ouro": {
      "minTotal": 471,
      "maxTotal": 530,
      "completude": 130,
      "label": "Ouro (Excelente)",
      "criterio": "Total >= 471"
    }
  },
  "metrics": {
    "encontrabilidade": [
      {
        "id": "dcat_title_quality",
        "property": "dcat:title",
        "dimension": "encontrabilidade",
        "validations": [
          {
            "id": "dcat_title_quality_check",
            "type": "property_quality",
            "label": "Título",
            "description": "Detectar caracteres repetitivos e no mínimo 5 caracteres. Princípios FAIR: F2, F4.",
            "weight": 25,
            "check": {
              "minLength": 5,
              "detectRepetitive": true
            }
          }
        ]
      },
      {
        "id": "dct_description_quality",
        "property": "dcterms:description",
        "dimension": "encontrabilidade",
        "validations": [
          {
            "id": "dct_description_quality_check",
            "type": "property_quality",
            "label": "Descrição",
            "description": "Detectar caracteres repetitivos e no mínimo 200 caracteres. Princípios FAIR: F2, R1.",
            "weight": 25,
            "check": {
              "minLength": 200,
              "detectRepetitive": true
            }
          }
        ]
      },
      {
        "id": "dcat_theme_present",
        "property": "dcat:theme",
        "dimension": "encontrabilidade",
        "obrigatorio_em": "Bronze",
        "validations": [
          {
            "id": "dcat_theme_vocabulary",
            "type": "vocabulary_check",
            "label": "Tema",
            "description": "Verifica se algum dos temas utilizados pertence ao VCR (Vocabulário Controlado de Referência). Princípios FAIR: F4, I2.",
            "weight": 30,
            "vocabularies": [
              "https://dcat-br.github.io/dcat-br/docs/vocabularies/themes/themes.csv"
            ]
          }
        ]
      },
      {
        "id": "dct_spatial_present",
        "property": "dct:spatial",
        "dimension": "encontrabilidade",
        "obrigatorio_em": "Prata",
        "validations": [
          {
            "id": "dct_spatial_vocabulary",
            "type": "vocabulary_check",
            "label": "Pesquisa espacial",
            "description": "Cobertura espacial pertence ao VCR. Princípios FAIR: F2, I3.",
            "weight": 20,
            "check": {
              "minCount": 1
            }
          }
        ]
      },
      {
        "id": "dct_temporal_present",
        "property": "dcat:startDate",
        "dimension": "encontrabilidade",
        "obrigatorio_em": "Prata",
        "validations": [
          {
            "id": "dct_temporal_valid",
            "type": "temporal_check",
            "label": "Pesquisa temporal",
            "description": "dcat:startDate e dcat:endDate em ISO 8601 válido, com início ≤ fim. Princípios FAIR: F2.",
            "weight": 20,
            "check": {
              "properties": [
                "dcat:startDate",
                "dcat:endDate"
              ],
              "minCount": 1,
              "iso8601": true,
              "startBeforeEnd": true
            }
          }
        ]
      }
    ],
    "acessibilidade": [
      {
        "id": "dcat_accessURL_valid",
        "property": "dcat:accessURL",
        "dimension": "acessibilidade",
        "obrigatorio_em": "Prata",
        "validations": [
          {
            "id": "dcat_accessURL_is_valid",
            "type": "url_check",
            "label": "Acessibilidade da URL de acesso",
            "description": "Protocolo padronizado (HTTP/HTTPS) e resposta válida (2xx/3xx). Princípios FAIR: A1, A1.1, A2.",
            "weight": 100,
            "check": {
              "minCount": 1,
              "statusCodes": [
                200, 201, 202, 203, 204, 205, 206, 207, 208, 226,
                300, 301, 302, 303, 304, 305, 306, 307, 308
              ]
            }
          }
        ]
      }
    ],
    "interoperabilidade": [
      {
        "id": "dct_format_vocabulary",
        "property": "dct:format",
        "dimension": "interoperabilidade",
        "obrigatorio_em": "Prata",
        "validations": [
          {
            "id": "dct_format_vocabulary_check",
            "type": "vocabulary_check",
            "label": "Formato",
            "description": "Formato pertence ao vocabulário controlado (ex: lista IANA). Princípios FAIR: I1, I2.",
            "weight": 15,
            "vocabularies": [
              "https://dcat-br.github.io/dcat-br/docs/vocabularies/formato/formatos.csv"
            ]
          },
          {
            "id": "dct_format_nonproprietary",
            "type": "format_check",
            "label": "Formato não proprietário",
            "description": "Classificação por VCR (não proprietário). Princípios FAIR: I1.",
            "weight": 40,
            "vocabularies": [
              "https://dcat-br.github.io/dcat-br/docs/vocabularies/formato/non_proprietary_formats.csv"
            ]
          },
          {
            "id": "dct_format_machine_readable",
            "type": "format_check",
            "label": "Formato legível por máquina",
            "description": "Classificação por VCR (machine-readable). Princípios FAIR: I1.",
            "weight": 40,
            "vocabularies": [
              "https://dcat-br.github.io/dcat-br/docs/vocabularies/formato/machine_readable_formats.csv"
            ]
          }
        ]
      },
      {
        "id": "dcat_mediaType_present",
        "property": "dcat:mediaType",
        "dimension": "interoperabilidade",
        "obrigatorio_em": "Prata",
        "validations": [
          {
            "id": "dcat_mediaType_consistent",
            "type": "vocabulary_check",
            "label": "Tipo de mídia",
            "description": "Formato em VCR com mapeamento consistente para mediaType (ex.: CSV→text/csv). Penaliza divergências (ex.: format=CSV + mediaType=application/pdf). Princípios FAIR: I1, I2.",
            "weight": 15,
            "vocabularies": [
              "https://dcat-br.github.io/dcat-br/docs/vocabularies/formato/formatos.csv"
            ]
          }
        ]
      }
    ],
    "reutilizabilidade": [
      {
        "id": "dct_license_vocabulary",
        "property": "dct:license",
        "dimension": "reutilizabilidade",
        "obrigatorio_em": "Bronze",
        "validations": [
          {
            "id": "dct_license_vocabulary_check",
            "type": "vocabulary_check",
            "label": "Vocabulário da licença",
            "description": "Licença em vocabulário controlado. Princípios FAIR: R1.1.",
            "weight": 30,
            "vocabularies": [
              "https://dcat-br.github.io/dcat-br/docs/vocabularies/VCR-LU/licensa-uso.csv"
            ]
          }
        ]
      },
      {
        "id": "dct_accessRights_vocabulary",
        "property": "dct:accessRights",
        "dimension": "reutilizabilidade",
        "obrigatorio_em": "Bronze",
        "validations": [
          {
            "id": "dct_accessRights_vocabulary_check",
            "type": "vocabulary_check",
            "label": "Observância Legal (Access Rights)",
            "description": "Valor em vocabulário controlado (SEI). Princípios FAIR: R1.1, R1.",
            "weight": 15,
            "vocabularies": [
              "https://dcat-br.github.io/dcat-br/docs/vocabularies/SEI/sei.csv"
            ]
          }
        ]
      },
      {
        "id": "dcat_contactPoint_present",
        "property": "dcat:contactPoint",
        "dimension": "reutilizabilidade",
        "obrigatorio_em": "Bronze",
        "validations": [
          {
            "id": "dcat_contactPoint_is_present",
            "type": "property_exists",
            "label": "Ponto de contato",
            "description": "Contato vinculável ao SIORG (departamento) e e-mail com domínio institucional compatível com órgão. Princípios FAIR: R1.2.",
            "weight": 20,
            "check": {
              "minCount": 1
            }
          }
        ]
      },
      {
        "id": "dct_publisher_present",
        "property": "dct:publisher",
        "dimension": "reutilizabilidade",
        "obrigatorio_em": "Bronze",
        "validations": [
          {
            "id": "dct_publisher_siorg",
            "type": "property_exists",
            "label": "Publicador",
            "description": "SIORG resolvível e consistência com organização/área técnica (não contraditórios). Princípios FAIR: R1.2.",
            "weight": 10,
            "check": {
              "minCount": 1
            }
          }
        ]
      }
    ],
    "contextualidade": [
      {
        "id": "dcat_spatialResolutionInMeters",
        "property": "dcat:spatialResolutionInMeters",
        "dimension": "contextualidade",
        "obrigatorio_em": "Ouro",
        "validations": [
          {
            "id": "dcat_spatialResolution_vocabulary",
            "type": "vocabulary_check",
            "label": "Granularidade espacial",
            "description": "Granularidade em lista controlada (município/UF/país ou resolução em metros). Princípios FAIR: F2, I3, C.",
            "weight": 20,
            "check": {
              "minCount": 1
            }
          }
        ]
      }
    ]
  },
  "completude": {
    "bronze": {
      "totalPoints": 83,
      "fields": {
        "dcterms:title": { "points": 10, "required": true, "description": "Título: nome por extenso do conjunto de dados." },
        "dcterms:description": { "points": 10, "required": true, "description": "Descrição: resumo das principais informações e políticas públicas impactadas." },
        "dcat:keyword": { "points": 8, "required": true, "description": "Palavras-chave: termos para indexação e busca." },
        "dcterms:creator": { "points": 5, "required": true, "description": "Organização: órgão responsável (vinculado ao SIORG)." },
        "dcterms:publisher": { "points": 7, "required": true, "description": "Área técnica responsável: unidade administrativa responsável." },
        "dcat:contactPoint": { "points": 8, "required": true, "description": "E-mail de contato: canal de comunicação com a área técnica." },
        "dcterms:accrualPeriodicity": { "points": 10, "required": true, "description": "Periodicidade: frequência de atualização (usar vocabulário controlado)." },
        "dcat:version": { "points": 2, "required": true, "description": "Versão: indicação da versão (ex: SemVer)." },
        "dcterms:license": { "points": 8, "required": true, "description": "Licença de uso: Creative Commons ou outra licença definida." },
        "dcterms:accessRights": { "points": 8, "required": true, "description": "Observância legal: se o dado é público, restrito ou sigiloso." },
        "dcat:theme": { "points": 7, "required": true, "description": "Temas: categoria extraída do Vocabulário Controlado do Governo Eletrônico (VCGE)." }
      }
    },
    "prata": {
      "totalPoints": 125,
      "additionalPoints": 42,
      "datasetFields": {
        "dcterms:spatial": { "points": 5, "required": true, "description": "Cobertura espacial: extensão geográfica dos dados (ex: links do IBGE)." },
        "dcat:startDate": { "points": 2, "required": true, "description": "Cobertura temporal: data inicial do período coberto pelos dados." },
        "dcat:endDate": { "points": 2, "required": true, "description": "Cobertura temporal: data final do período coberto pelos dados." }
      },
      "resourceFields": {
        "dcat:title": { "points": 9, "required": true, "description": "Título do recurso: nome específico do arquivo ou API." },
        "dcat:description": { "points": 9, "required": true, "description": "Descrição do recurso: detalhes sobre o arquivo específico." },
        "dcat:accessURL": { "points": 9, "required": true, "description": "URL de acesso: link onde o recurso está hospedado." },
        "dcterms:type": { "points": 3, "required": true, "description": "Tipo do recurso: se é arquivo (CSV, PDF) ou serviço (API)." },
        "dcterms:format": { "points": 3, "required": true, "description": "Formato: formato técnico (ex: CSV, JSON) definido pela IANA." }
      }
    },
    "ouro": {
      "totalPoints": 130,
      "additionalPoints": 5,
      "datasetFields": {
        "dcat:spatialResolutionInMeters": { "points": 5, "required": true, "description": "Granularidade espacial: nível de detalhe (ex: municipal, estadual)." }
      }
    }
  }
}
